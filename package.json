{
  "name": "ws-front",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "next build src",
    "start": "NODE_ENV=production node server.js",
    "eslint:lint": "./node_modules/.bin/eslint src --fix",
    "prettier": "./node_modules/.bin/prettier \"./src/**/*.+(js|jsx)\" ",
    "diff:prettier": "yarn prettier -- --list-different",
    "format": "yarn prettier -- --write",
    "validate": "yarn eslint:lint && yarn diff:prettier",
    "precommit": "yarn validate",
    "storybook": "cross-env NODE_PATH=src start-storybook -p 9001 -c .storybook -s ./src/",
    "storybook:build": "cross-env NODE_PATH=src/ build-storybook -c .storybook -o storybook/storybook -s ./src/ && sed -i -e 's/\\/sb_dll/sb_dll/g' storybook/storybook/index.html",
    "storybook:serve": "serve -p 5000 storybook/",
    "test:watch": "NODE_ENV=test NODE_PATH=src jest --watch",
    "test": "NODE_ENV=test NODE_PATH=src jest"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "apollo-boost": "^0.1.16",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "dotenv-webpack": "^1.6.0",
    "eslint-plugin-babel": "^5.3.0",
    "express": "^4.16.4",
    "focus-trap": "^4.0.2",
    "formik": "^1.4.3",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.4",
    "intersection-observer": "^0.5.1",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.11",
    "merge-graphql-schemas": "^1.5.8",
    "next": "^9.3.2",
    "next-routes": "^1.4.2",
    "next-with-apollo": "^3.3.2",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.4",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.8.4",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@storybook/addon-actions": "^4.1.6",
    "@storybook/addon-backgrounds": "^4.1.9",
    "@storybook/addon-knobs": "^4.1.11",
    "@storybook/addon-links": "^4.1.6",
    "@storybook/addon-viewport": "^4.1.11",
    "@storybook/addons": "^4.1.6",
    "@storybook/react": "^4.1.6",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.8.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "1.2.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-import-resolver-babel-module": "^5.0.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-module-resolver": "^0.8.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "husky": "^1.3.1",
    "jest": "23.5.0",
    "prettier": "^1.15.3",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.4.2",
    "react-testing-library": "^5.4.2",
    "serve": "^10.1.1"
  },
  "license": "ISC"
}
